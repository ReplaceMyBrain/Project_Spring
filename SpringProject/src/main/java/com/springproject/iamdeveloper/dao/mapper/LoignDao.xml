<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "com.springproject.iamdeveloper.dao.LoginDao">
	
	<select id="loginDao" resultType="com.springproject.iamdeveloper.dto.UserDto">
		SELECT email, admin, deletedate FROM user WHERE email=#{param1} and pwd=#{param2}
	</select>
	
</mapper>